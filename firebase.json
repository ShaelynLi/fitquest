{
  "hosting": {
    "public": "web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api{,/**}",
        "run": {
          "serviceId": "fitquest-api",
          "region": "australia-southeast1"
        }
      }
    ]
  }
}
